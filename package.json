{
  "name": "image-processing-api",
  "version": "2.0.0",
  "description": "Image Processing API Using Sharp",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "start:prod": "npm run build && npm run start",
    "dev": "nodemon --watch ts-node src/server.ts ",
    "lint": "eslint 'src/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "jasmine": "jasmine",
    "test": "npm run build && npm run jasmine"
  },
  "keywords": [
    "Udacity",
    "Image-Processing-api",
    "nodejs",
    "typescript",
    "express",
    "sharp",
    "jasmine",
    "prettier",
    "eslint",
    "ejs",
    "supertest"
  ],
  "author": "Khalid Mesbah",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/node": "^17.0.18",
    "@types/sharp": "^0.29.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "ejs": "^3.1.9",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "supertest": "^6.2.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.17.2",
    "sharp": "^0.30.1"
  }
}